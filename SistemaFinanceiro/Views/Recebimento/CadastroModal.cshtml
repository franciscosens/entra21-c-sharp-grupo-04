@{
    Layout = null;
}


<div class="modal fade" id="cadastrar-recebimento-modal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    <span>X</span>
                </button>
                <h4>Cadatrar recebimento</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label class="col-md-2 control-label" for="campo-valor-recebido">Valor recebido<h11>*</h11></label>
                    <input id="campo-valor-recebido" name="Recebimentos.Valor_recebido" placeholder="" class="form-control input-md col-sm-6" required="" type="text">
                </div>

                <div class="form-group">
                    <label class="col-md-2 control-label" for="campo-data-recebimento">Data de recebimento<h11>*</h11></label>
                    <input id="campo-data-recebimento" name="Reebimentos.Data_recebimento" placeholder="" class="form-control input-md col-sm-6" required="" type="text">
                </div>
                <div class="form-group">
                    <label class="col-md-2 control-label" for="campo-descricao">Data de recebimento<h11>*</h11></label>
                    <input id="campo-descricao" name="Reebimentos.Descricao" placeholder="" class="form-control input-md col-sm-6" required="" type="text">
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger" data-dismiss="modal" id="cancelar-cadastro-recebimento">Cancelar</button>
                    <input type="submit" class="btn btn-primary" value="Salvar" id="salvar-cadastro-modal">
                </div>


            </div>
        </div>
    </div>
</div>

