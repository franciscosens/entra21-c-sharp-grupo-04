@{
    Layout = null;
}


<div class="modal fade" id="editar-recebimento-modal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    <span>X</span>
                </button>
                <h4>Editar recebimentos</h4>
            </div>
            <input type="hidden" id="campo-id-recebimento" />
            <div class="modal-body">
                <div class="form">
                    <label class="col-md-2 control-label" for="campo-recebimento-valor">Valor recebido<h11>*</h11></label>
                    <input id="campo-recebimento-valor" name="campo-recebimento-valor-recebido" placeholder="" class="form-control input-md col-sm-6" required="" type="text">
                </div>

                <div class="form">
                    <label class="col-md-2 control-label" for="campo-recebimento-data">Data de recebimento<h11>*</h11></label>
                    <input id="campo-recebimento-data" name="campo-recebimento-data-recebimento" placeholder="" class="form-control input-md col-sm-6" required="" type="text">
                </div>
                

                <div class="modal-footer">
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>
                    <input type="submit" class="btn btn-success" value="Salvar" id="salvar-recebimento-cadastro-modal">
                </div>
            </div>
        </div>
    </div>
</div>

